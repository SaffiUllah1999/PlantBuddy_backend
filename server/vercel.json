{
    "version": 2,
    "builds": [
        { "src": "server/index.js", "use": "@vercel/node" }
    ],
    "functions": {
        "server/index.js": {
            "runtime": "nodejs18.x",
            "memory": 1024,
            "maxDuration": 30
        }
    },
    "routes": [
        { "src": "/(.*)", "dest": "/server/index.js" }
    ]
}